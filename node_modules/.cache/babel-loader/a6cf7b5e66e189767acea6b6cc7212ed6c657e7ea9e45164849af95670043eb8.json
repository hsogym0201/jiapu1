em => item.姓氏 === surname && item.居地 === this.selectedProvince);\n      const versionData = [];\n      surnameData.forEach(item => {\n        const version = item.版本年代;\n        const count = item.数量;\n        const existingVersion = versionData.find(v => v.version === version);\n        if (existingVersion) {\n          existingVersion.count += count;\n        } else {\n          versionData.push({\n            version,\n            count\n          });\n        }\n      });\n      versionData.sort((a, b) => a.version - b.version);\n      return versionData;\n    }\n  },\n  mounted() {\n    this.words = this.getTop20Surnames(this.selectedProvince); // 初始化词云数据\n  }\n};","map":{"version":3,"names":["XingShiXuanZe","DiTu1","CiYun","DiTu2","ZheXianTu","ZhuZhuangTu","jsonData","require","name","components","data","menuItems","activeIndex","words","versionData","clickedWord","showPopup","selectedProvince","methods","setActive","index","getTop20Surnames","province","surnameCounts","forEach","item","居地","姓氏","sortedSurnames","Object","entries","sort","a","b","map","light","handleWordClick","word","getSurnameVersionData","$refs","zheXianTu","updateChart","handleProvinceClick","surname","surnameData","filter","version","版本年代","count","数量","existingVersion","find","v","push","mounted"],"sources":["D:\\下载\\jiapudemo1\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>寻根问祖，探索家族的历史脉络</h1>\n    <h3>每个姓氏背后都有一段深厚的历史与文化...</h3>\n\n    <div class=\"navbar\">\n      <div class=\"navbar-title\">\n        寻根问祖 <span>可以看见的家谱可视化平台</span>\n      </div>\n      <div class=\"navbar-menu\">\n        <a\n            v-for=\"(item, index) in menuItems\"\n            :key=\"index\"\n            href=\"#\"\n            :class=\"['menu-item', { active: activeIndex === index }]\"\n            @click=\"setActive(index)\"\n        >\n          {{ item }}\n        </a>\n      </div>\n    </div>\n\n    <h4>家谱地图展示--了解你的姓氏在全国各地的家族故事</h4>\n    <div class=\"horizontal-container\">\n      <XingShiXuanZe />\n      <DiTu1 />\n    </div>\n\n    <hr />\n    <h4>地域姓氏构成对比--探索各地的家族足迹</h4>\n    <div class=\"horizontal-container\">\n      <CiYun :wordArr=\"words\" @word-clicked=\"handleWordClick\" />\n      <DiTu2 @province-clicked=\"handleProvinceClick\" />\n    </div>\n\n    <hr />\n    <h4>姓氏时空上的分布趋势--深入家族传承之路与繁衍之势</h4>\n    <ZheXianTu ref=\"zheXianTu\" :versionData=\"versionData\" />\n    <hr />\n    <ZhuZhuangTu />\n  </div>\n</template>\n\n<script>\nimport XingShiXuanZe from './components/XingShiXuanZe.vue';\nimport DiTu1 from './components/DiTu1.vue';\nimport CiYun from '@/components/CiYun.vue';\nimport DiTu2 from '@/components/DiTu2.vue';\nimport ZheXianTu from '@/components/ZheXianTu.vue';\nimport ZhuZhuangTu from '@/components/ZhuZhuangTu.vue';\n\nconst jsonData = require('./assets/new.json');\n\nexport default {\n  name: 'App',\n  components: {\n    XingShiXuanZe,\n    DiTu1,\n    CiYun,\n    DiTu2,\n    ZheXianTu,\n    ZhuZhuangTu,\n  },\n  data() {\n    return {\n      menuItems: ['家谱地图展示', '地域姓氏构成对比', '姓氏时空上的分布趋势'],\n      activeIndex: 0,\n      words: [], // 词云数据\n      versionData: [], // 版本年代数据\n      clickedWord: null,\n      showPopup: false,\n      selectedProvince: '山东', // 默认选中省份\n    };\n  },\n  methods: {\n    setActive(index) {\n      this.activeIndex = index;\n    },\n    getTop20Surnames(province) {\n      const surnameCounts = {};\n      jsonData.forEach((item) => {\n        if (item.居地 === province) {\n          surnameCounts[item.姓氏] = (surnameCounts[item.姓氏] || 0) + 1;\n        }\n      });\n\n      const sortedSurnames = Object.entries(surnameCounts).sort((a, b) => b[1] - a[1]);\n      return sortedSurnames.map(([name]) => ({ name, light: true }));\n    },\n    handleWordClick(word) {\n      // 获取当前姓氏的版本数据\n      const versionData = this.getSurnameVersionData(word.name);\n      // 调用折线图组件的更新方法\n      this.$refs.zheXianTu.updateChart(versionData);\n    },\n    handleProvinceClick(province) {\n      this.selectedProvince = province;\n      this.words = this.getTop20Surnames(province);\n    },\n    getSurnameVersionData(surname) {\n      const surnameData = jsonData.filter(item => item.姓氏 === surname && item.居地 === this.selectedProvince);\n      const versionData = [];\n      surnameData.forEach(item => {\n        const version = item.版本年代;\n        const count = item.数量;\n        const existingVersion = versionData.find(v => v.version === version);\n        if (existingVersion) {\n          existingVersion.count += count;\n        } else {\n          versionData.push({ version, count });\n        }\n      });\n      versionData.sort((a, b) => a.version - b.version);\n      return versionData;\n    },\n  },\n\n  mounted() {\n    this.words = this.getTop20Surnames(this.selectedProvince); // 初始化词云数据\n  },\n};\n</script>\n\n<style scoped>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\nh1 {\n  font-family: 'Georgia', serif;\n  font-size: 2.5rem;\n  color: #2c3e50;\n  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);\n  margin-bottom: 20px;\n}\nh3 {\n  font-family: 'Arial', sans-serif;\n  font-size: 1.2rem;\n  color: #34495e;\n  line-height: 1.8;\n  max-width: 800px;\n  margin: 0 auto;\n  text-align: justify;\n}\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 10px 20px;\n  background-color: #fff;\n  border-bottom: 1px solid #ddd;\n  font-family: 'Arial', sans-serif;\n  margin-left: 200px;\n  border: none;\n}\n.navbar-title {\n  font-size: 1.5rem;\n  font-weight: bold;\n  color: #000;\n}\n.navbar-menu {\n  display: flex;\n  gap: 20px;\n  margin-right: 200px;\n}\n.menu-item {\n  text-decoration: none;\n  color: #333;\n  font-size: 1rem;\n  padding: 5px 10px;\n  position: relative;\n  transition: color 0.3s ease;\n}\n.menu-item:hover {\n  color: #f60;\n}\n.menu-item.active {\n  color: #f60;\n  font-weight: bold;\n}\n.horizontal-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 200px;\n  padding: 10px;\n}\n</style>\n"],"mappings":";;;;;AA4CA,OAAOA,aAAY,MAAO,gCAAgC;AAC1D,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,KAAI,MAAO,wBAAwB;AAC1C,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,WAAU,MAAO,8BAA8B;AAEtD,MAAMC,QAAO,GAAIC,OAAO,CAAC,mBAAmB,CAAC;AAE7C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVT,aAAa;IACbC,KAAK;IACLC,KAAK;IACLC,KAAK;IACLC,SAAS;IACTC;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CAAC,QAAQ,EAAE,UAAU,EAAE,YAAY,CAAC;MAC/CC,WAAW,EAAE,CAAC;MACdC,KAAK,EAAE,EAAE;MAAE;MACXC,WAAW,EAAE,EAAE;MAAE;MACjBC,WAAW,EAAE,IAAI;MACjBC,SAAS,EAAE,KAAK;MAChBC,gBAAgB,EAAE,IAAI,CAAE;IAC1B,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,KAAK,EAAE;MACf,IAAI,CAACR,WAAU,GAAIQ,KAAK;IAC1B,CAAC;IACDC,gBAAgBA,CAACC,QAAQ,EAAE;MACzB,MAAMC,aAAY,GAAI,CAAC,CAAC;MACxBjB,QAAQ,CAACkB,OAAO,CAAEC,IAAI,IAAK;QACzB,IAAIA,IAAI,CAACC,EAAC,KAAMJ,QAAQ,EAAE;UACxBC,aAAa,CAACE,IAAI,CAACE,EAAE,IAAI,CAACJ,aAAa,CAACE,IAAI,CAACE,EAAE,KAAK,CAAC,IAAI,CAAC;QAC5D;MACF,CAAC,CAAC;MAEF,MAAMC,cAAa,GAAIC,MAAM,CAACC,OAAO,CAACP,aAAa,CAAC,CAACQ,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAAC,CAAC,IAAID,CAAC,CAAC,CAAC,CAAC,CAAC;MAChF,OAAOJ,cAAc,CAACM,GAAG,CAAC,CAAC,CAAC1B,IAAI,CAAC,MAAM;QAAEA,IAAI;QAAE2B,KAAK,EAAE;MAAK,CAAC,CAAC,CAAC;IAChE,CAAC;IACDC,eAAeA,CAACC,IAAI,EAAE;MACpB;MACA,MAAMvB,WAAU,GAAI,IAAI,CAACwB,qBAAqB,CAACD,IAAI,CAAC7B,IAAI,CAAC;MACzD;MACA,IAAI,CAAC+B,KAAK,CAACC,SAAS,CAACC,WAAW,CAAC3B,WAAW,CAAC;IAC/C,CAAC;IACD4B,mBAAmBA,CAACpB,QAAQ,EAAE;MAC5B,IAAI,CAACL,gBAAe,GAAIK,QAAQ;MAChC,IAAI,CAACT,KAAI,GAAI,IAAI,CAACQ,gBAAgB,CAACC,QAAQ,CAAC;IAC9C,CAAC;IACDgB,qBAAqBA,CAACK,OAAO,EAAE;MAC7B,MAAMC,WAAU,GAAItC,QAAQ,CAACuC,MAAM,CAACpB,IAAG,IAAKA,IAAI,CAACE,EAAC,KAAMgB,OAAM,IAAKlB,IAAI,CAACC,EAAC,KAAM,IAAI,CAACT,gBAAgB,CAAC;MACrG,MAAMH,WAAU,GAAI,EAAE;MACtB8B,WAAW,CAACpB,OAAO,CAACC,IAAG,IAAK;QAC1B,MAAMqB,OAAM,GAAIrB,IAAI,CAACsB,IAAI;QACzB,MAAMC,KAA